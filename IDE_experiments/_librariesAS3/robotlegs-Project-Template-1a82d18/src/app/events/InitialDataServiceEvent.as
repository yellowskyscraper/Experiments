package app.events{	import flash.events.Event;	public class InitialDataServiceEvent extends Event	{		/*******************************************************************************************		*								public properties										   *		*******************************************************************************************/		public static const DATA_LOADED : String = "DATA_LOADED";		public static const DATA_LOADING_FAILED : String = "DATA_LOADING_FAILED";				/*******************************************************************************************		*								public methods											   *		*******************************************************************************************/		public function InitialDataServiceEvent(type : String)		{			super(type);		}				override public function clone() : Event		{			return new InitialDataServiceEvent(type);		}	}}