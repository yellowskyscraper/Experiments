package app.controller{	import app.events.URIEvent;	import app.services.SWFAddressService;	import org.robotlegs.mvcs.Command;	public class ChangeURICommand extends Command	{		/*******************************************************************************************		*								public properties										   *		*******************************************************************************************/		[Inject] public var event : URIEvent;		[Inject] public var service : SWFAddressService;						/*******************************************************************************************		*								public methods											   *		*******************************************************************************************/		override public function execute() : void		{			service.requestURI(event.uri);		}	}}