package app.controller.startup{	import app.services.InitialDataService;	import app.view.ApplicationView;	import org.robotlegs.base.ContextEvent;	import org.robotlegs.mvcs.Command;	/**	 * @author tschneidereit	 */	public class StartupCommand extends Command	{		/*******************************************************************************************		*								public properties										   *		*******************************************************************************************/		[Inject] public var startupService : InitialDataService;				/*******************************************************************************************		*								protected/ private properties							   *		*******************************************************************************************/						/*******************************************************************************************		*								public methods											   *		*******************************************************************************************/		public function StartupCommand()		{		}		override public function execute() : void		{			contextView.addChild(new ApplicationView());			startupService.loadInitialData();			dispatch(new ContextEvent(ContextEvent.STARTUP_COMPLETE));		}	}}